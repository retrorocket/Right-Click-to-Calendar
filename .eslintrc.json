{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "webextensions": true,
    "jest/globals": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "babelOptions": {
      "plugins": [
        "importAssertions"
      ]
    }
  },
  "rules": {},
  "globals": {
    "luxon": false
  },
  "plugins": [
    "jest",
    "@babel/plugin-syntax-import-assertions"
  ]
}
